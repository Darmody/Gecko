    "lru-memoize": "^1.0.0",
    "map-props": "^1.0.0",
    "material-ui": "^0.13.3",
    "mdi": "^1.3.41",
    "multireducer": "^1.0.2",
    "pretty-error": "^1.2.0",
    "query-string": "^3.0.0",
    "react": "^0.14.2",
    "react-document-meta": "^2.0.0",
    "react-dom": "^0.14.1",
    "react-hotkey": "^0.5.0",
    "react-hotkeys": "^0.7.0",
    "react-inline-css": "^2.0.0",
    "react-mixin": "^3.0.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
